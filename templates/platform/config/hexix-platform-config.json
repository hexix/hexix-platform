{
    "id": "docker",
    "title": "Docker Actions:",
    "actions": {
        "env": {
            "command": "env",
            "launchType": "fore",
            "title": "Show the environment vars"
        },
        "up": {
            "command": "docker-compose --verbose up",
            "launchType": "back",
            "title": "Build/Bring docker services up"
        },
        "down": {
            "command": "docker-compose down",
            "launchType": "fore",
            "title": "Bring docker services down"
        },
        "versions": {
            "subActions": [
                [
                    "version-docker"
                ],
                [
                    "version-docker-compose"
                ]
            ],
            "title": "Docker Versions"
        },
        "version-docker": {
            "command": "docker -v",
            "launchType": "fore",
            "title": "Show docker version",
            "showUsage": false
        },
        "version-docker-compose": {
            "command": "docker-compose version",
            "launchType": "fore",
            "title": "Show docker-compose version",
            "showUsage": false
        },
        "restart": {
            "command": "docker-compose kill && docker-compose start",
            "launchType": "fore",
            "title": "Restart docker services"
        }
    }
}
