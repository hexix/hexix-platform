{
  "name": "hexix-platform",
  "version": "0.1.5",
  "description": "Hexix Platform",
  "main": "hexix-platform.js",
  "directories": {
    "bin": "bin",
    "config": "config",
    "src": "src"
  },
  "scripts": {
    "test": "jest --no-cache",
    "test-debug": "node --nolazy --debug-brk node_modules/jest/bin/jest --no-cache -i --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexix/hexix-platform.git"
  },
  "keywords": [
    "development",
    "environment",
    "nodejs",
    "docker"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "command-line-args": "^4.0.6",
    "command-line-commands": "^2.0.0",
    "command-line-usage": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "copy-template-dir": "^1.3.0"
  },
  "author": "Greg Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hexix/hexix-platform/issues"
  },
  "homepage": "https://github.com/hexix/hexix-platform#readme",
  "devDependencies": {
    "jest": "^20.0.4"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "<rootDir>/__tests__"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  }
}